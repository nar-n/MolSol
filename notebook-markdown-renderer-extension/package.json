{
  "name": "notebook-markdown-renderer-extension",
  "displayName": "Notebook Markdown Renderer Extension",
  "description": "An extension to render markdown in notebooks.",
  "version": "0.0.1",
  "publisher": "your-name",
  "engines": {
    "vscode": "^1.60.0"
  },
  "activationEvents": [
    "onNotebookEditor:markdown"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "notebookRenderer": [
      {
        "id": "markdownRenderer",
        "displayName": "Markdown Renderer",
        "mimeTypes": ["text/markdown"],
        "provider": {
          "type": "markdownRenderer",
          "script": "./out/markdownRenderer.js"
        }
      }
    ]
  },
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "watch": "tsc -watch -p tsconfig.json",
    "prepublish": "npm run build"
  },
  "devDependencies": {
    "typescript": "^4.0.0",
    "vscode": "^1.60.0",
    "eslint": "^7.0.0"
  },
  "dependencies": {}
}